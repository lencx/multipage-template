{
  "name": "multipage-template",
  "version": "1.1.1",
  "description": "TypeScript + Pug + Scss + Webpack",
  "repository": "git@github.com:lencx/multipage-template.git",
  "author": "Len <cxin1314@gmail.com>",
  "scripts": {
    "clear": "shx rm -rf dist && shx echo Clear Dist File DONE!",
    "dev": "cross-env NODE_ENV=develpoment webpack-dev-server -d --config webpack/webpack.dev.babel.js",
