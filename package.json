    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.0.6",
    "lost": "^8.2.1",
    "node-sass": "^4.7.2",
    "postcss": "^6.0.19",
    "postcss-cssnext": "^3.1.0",
    "postcss-loader": "^2.1.1",
    "progress-bar-webpack-plugin": "^1.11.0",
    "pug": "^2.0.0",
    "pug-html-loader": "^1.1.5",
    "sass-loader": "^6.0.7",
    "shx": "^0.2.2",
    "style-loader": "^0.20.2",
    "url-loader": "^1.0.1",
    "webpack": "^4.1.0",
    "webpack-cli": "^2.0.10",
    "webpack-dev-server": "^3.1.0"
